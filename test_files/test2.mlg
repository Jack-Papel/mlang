let is_prime = | num ~ num < 2 : false
               | num : 2..(num - 1) &&& 
                 | factor : num % factor != 0

0..100 
  # is_prime 
  @ (| p : p * p)
  $ print

1..101 @ (
  | num ~ num % 15 == 0: "fizzbuzz"
  | num ~ num % 3 == 0: "fizz"
  | num ~ num % 5 == 0: "buzz"
  | num : num
  ) $ print

1..101 $ | num :
           let out = ""
           (|~ num % 3 == 0:
              out = out + "fizz")
           (|~ num % 5 == 0: 
              out = out + "buzz")
           (|~ out == "":
              out = num)
           out print