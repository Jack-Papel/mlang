let is_prime = | num ~ num < 2 : false
               | num : 2..(num - 1) &&& 
                 | factor : num % factor != 0

0..100 
  # is_prime 
  @ (| p : p * p)
  $ print

1..101 $ | num ~ num % 15 == 0: "fizzbuzz" print
         | num ~ num % 3 == 0: "fizz" print
         | num ~ num % 5 == 0: "buzz" print
         | num : num print