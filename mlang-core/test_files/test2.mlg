"Printing the prime numbers from 1 - 100, squared" println

let is_prime = | num ~ num < 2 : false
               | num : 2..(num - 1) &&& 
                 | factor : num % factor != 0

0..100 
  # is_prime 
  @ (| p : p * p)
  $ println

"\nPrinting the fibonacci numbers" println

1..101 $ | num :
           let out = ""
           (|~ num % 3 == 0: out = out + "fizz")
           (|~ num % 5 == 0: out = out + "buzz")
           (|~ out == "": out = num)
           out println